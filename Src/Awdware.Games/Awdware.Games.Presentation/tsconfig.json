{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@gah/Awdware.Core.Presentation/*": [
        "projects/awdware-games/src/.gah/dependencies/Awdware.Core.Presentation/public-api"
      ],
      "@gah/Awdware.Shared.Presentation/*": [
        "projects/awdware-games/src/.gah/dependencies/Awdware.Shared.Presentation/public-api"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": false
  }
}